{{#each this}}
    <section class="module" id="{{lectureSlug this}}">
    <h1>{{Title}}</h1>

        {{#each Events}}
            <section>
                <div class="label"><h1>{{Label}} <span class="date">{{shortDate @key}}</span></h1></div>
                <div class="topics">
                    <ul>
                        {{#each Topics}}
                            <li>{{@key}}</li>
                        {{/each}}
                    </ul>
                </div>
                <div class="reading icon">
                    <h1>References</h1>

                    <ul>{{> _reading }}</ul>
                </div>
                <div class="slides icon">
                    <h1>Slides and Notes</h1>
                    <ul>
                        {{#each [Slides and Notes]}}
                            <li>{{maybeLink this}}</li>
                        {{/each}}
                    </ul>
                </div>
            </section>
        {{/each}}
    </section>
{{/each}}
